    </offsets>
  </model>
  
  <model>
    <path>Aircraft/Tecnam-P2010/Models/Interior/Panel/Instruments/switch/taxi-lights.xml</path>
    <offsets>
      <x-m>  0.023 </x-m>
      <y-m>  0.016 </y-m>
      <z-m> -0.200 </z-m>
