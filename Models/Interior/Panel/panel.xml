  </model>
  
  <model>
    <path>Aircraft/Tecnam-P2010/Models/Interior/Panel/Instruments/switch/other-lights.xml</path>
    <offsets>
      <x-m>  0.023 </x-m>
      <y-m>  0.076 </y-m>
      <z-m> -0.200 </z-m>
      <pitch-deg> -25 </pitch-deg>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/Tecnam-P2010/Models/Interior/Panel/Instruments/switch/other-lights.xml</path>
    <offsets>
      <x-m>  0.023 </x-m>
      <y-m>  0.106 </y-m>
      <z-m> -0.200 </z-m>
      <pitch-deg> -25 </pitch-deg>
