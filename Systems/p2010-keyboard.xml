<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <key n="100">
    <name>d</name>
    <desc>Pilot door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.crew.toggle();</script>
    </binding>
  </key>
  
  <key n="68">
    <name>D</name>
    <desc>Copilot door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.passenger.toggle();</script>
    </binding>
  </key>
  
  <key n="101">
    <name>e</name>
    <desc>Left passenger door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.passenger2.toggle();</script>
    </binding>
  </key>
  
  <key n="69">
    <name>E</name>
    <desc>Right passenger  door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.passenger3.toggle();</script>
    </binding>
  </key>
  
</PropertyList>
